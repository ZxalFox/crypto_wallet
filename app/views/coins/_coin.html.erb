<div id="<%= dom_id coin %>">
    <p>
        <strong>Description:</strong>
        <%= coin.description %>
    </p>

    <p>
        <strong>Acronym:</strong>
        <%= coin.acronym %>
    </p>

    <p>
        <strong>Image (url):</strong>
        <img src="<%= coin.url_image %>" width="50px" height="50px" />
    </p>
</div>